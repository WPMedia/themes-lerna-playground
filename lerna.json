{
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "command": {
    "publish": {
      "ignoreChanges": [
        "package.json",
        "CHANGELOG.md"
      ],
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "registry": "https://npm.pkg.github.com",
      "allowBranch": [
        "stable",
        "canary",
        "arc-themes-release-1.19"
      ]
    }
  }
}
